<%@ taglib uri="/tags/struts-html" prefix="html" %><%@ taglib uri="/tags/struts-bean"   prefix="bean" %><html><head><title>Eliminar Reservacion</title><link type="text/css" rel="stylesheet" rev="stylesheet" href="/room/css/style.css"></head><body background="/room/img/fondoa.jpg"><table align="center" class="tableLine">		<tr>		<td width="600" height="125" valign="top">&nbsp;				</td>	</tr>	 <tr> 		<td align="right" height="100">			 <html:form action="EliminarReserv"> <table align="center" width="60%" cellspacing="0" cellpadding="5"> <tr><td colspan="2" class="Titulo1" align="center">Estas seguro que quieres eliminar la reservacion:</td> </tr>  <tr><td width="40%" align="right" class="Titulo3">Sala : </td><td class="datos"><bean:write name="EliminarReservFormBean" property="sala.nombre_sala"/> </td> </tr>  <tr><td align="right" class="Titulo3">Fecha-Hora: </td><td class="datos"><bean:write name="EliminarReservFormBean" property="reservacion.tiempo" format="yyyy/MM/dd  HH:mm"/> </td> </tr>  <tr><td align="right" class="Titulo3">Departamento : </td><td class="datos"> <bean:write name="EliminarReservFormBean" property="reservacion.departamento"/></td> </tr>   <tr><td align="right" class="Titulo3">Comentario : </td><td class="datos"><bean:write name="EliminarReservFormBean" property="reservacion.comentario"/></td> </tr>  <tr><td align="center" colspan="2"><html:submit value="Si" property="operacion" styleClass="estilo"/>&nbsp;&nbsp;&nbsp;<html:submit value="No" property="operacion" styleClass="estilo"/>  </td> </tr> </table> <html:hidden property="reservacion.id_reservas"/> </html:form>		</td>  	</tr>	<tr>		<td  height="20" colspan="3" bgcolor="#DADDE2" align="center" class="pie">      &nbsp; Notmusa 2007 | Derechos Reservados &reg;     </td>	</tr></table></body></html>